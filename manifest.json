{
  "manifest_version": 3,
  "name": "Enlarge your ChatGPT",
  "description": "Make code preview bigger on ChatGPT website",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default-icons": {
      "16": "images/eggplant-16.png",
      "32": "images/eggplant-32.png",
      "128": "images/eggplant-128.png"
    }
  },
  "icons": {
    "16": "images/eggplant-16.png",
    "32": "images/eggplant-32.png",
    "128": "images/eggplant-128.png"
  }
}